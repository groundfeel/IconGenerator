{
  "name": "icon_generator",
  "version": "1.3.0",
  "description": "A macOS app to generate app icons",
  "main": "dist/main.js",
  "repository": "https://github.com/onmyway133/IconGenerator",
  "author": "Khoa Pham",
  "license": "MIT",
  "scripts": {
    "start": "npm run babel && electron .",
    "babel": "babel ./src --out-dir ./dist --copy-files",
    "release": "electron-packager . --icon=Icon/Icon.icns"
  },
  "build": {
    "appId": "com.onmyway133.IconGenerator",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "msi"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "image-size": "^0.6.1",
    "material-ui": "^0.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-tap-event-plugin": "^3.0.2",
    "rimraf": "^2.6.1",
    "sharp": "^0.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^5.0.0",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.8.2"
  }
}
